{
	"expo": {
		"name": "My Todo List",
		"slug": "my-todos-app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/todo.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/todo.png",
				"backgroundColor": "#FFFFFF"
			},
			"package": "com.anandafarhan.todosapp"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		}
	}
}
